<div class='table-responsive'>
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <% csv_table.headers.each do |header| %>
          <th><%= header %></th>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <% csv_table.each do |row| %>
        <tr>
          <% row.each do |element| %>
            <td><%= element[1] %></td>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
